<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/landing.css")
    @Styles.Render("~/Content/content.css")
    @Styles.Render("~/Content/tippy.css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @RenderSection("Nav", required: false)
    <div class="content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Scripts/Index.js")
    @Scripts.Render("~/Scripts/tippy.js")
    @Scripts.Render("~/Scripts/zxcvbn.js")
    @RenderSection("scripts", required: false)
    <script src='https://www.google.com/recaptcha/api.js' type="text/javascript"></script>
</body>
</html>
